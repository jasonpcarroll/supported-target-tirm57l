    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdbtarget",
            "request": "launch",
            "name": "TI-RM57 Debug @EXECUTABLE_NAME@",
            "program": "@EXECUTABLE_PATH@",
            "gdb": "@CMAKE_C_DEBUGGER@",
            "target": {
                "type": "extended-remote",
                "host":"localhost",
                "port":"3333",
                "server": "@DEBUGGER_DIR@/bin/openocd",
                "serverParameters": ["-f", "@DEBUGGER_CONFIG_PATH@"]
            }
        }
    
    ]
}